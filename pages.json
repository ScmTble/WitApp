{
	"easycom": {
			"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
		},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/schedule/schedule",
			"style": {
				"navigationBarTitleText": "课表",
				"enablePullDownRefresh":true
			}
		},
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "主页"
			}
		},
		{
			"path": "pages/class/class",
			"style": {
				"navigationBarTitleText": "空教室"
			}
		},
		{
			"path": "pages/login/index",
			"style": {
				"navigationBarTitleText": "登录"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "WitApp",
		"navigationBarBackgroundColor": "#161616",
		"backgroundColor": "#161616"
	},
	"tabBar": {
	    "color": "#3f3f5f",
	    "selectedColor": "#ffffff",
	    "borderStyle": "black",
	    "backgroundColor": "#161616",
	    "list": [{
	        "pagePath": "pages/index/index",
	        "iconPath": "static/index.png",
	        "selectedIconPath": "static/index_s.png",
	        "text": "主页"
	    },
		{
		    "pagePath": "pages/schedule/schedule",
		    "iconPath": "static/sh.png",
		    "selectedIconPath": "static/sh_s.png",
		    "text": "课表"
		},
		{
	        "pagePath": "pages/class/class",
	        "iconPath": "static/class.png",
	        "selectedIconPath": "static/calss_s.png",
	        "text": "空教室"
	    }],
		"fontSize":"15px",
		"height":"60px"
	}
}
